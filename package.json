{
  "name": "raremetal.js",
  "version": "0.1.0",
  "description": "Calculate aggregation tests and meta-analysis of aggregation test results using score statistics and covariance matrices",
  "engines": {
    "node": "~8.9.4"
  },
  "main": "dist/raremetal.js",
  "bin": {
    "raremetal.js": "src/app/cli.js"
  },
  "scripts": {
    "test": "gulp",
    "lint": "gulp lint",
    "test-browser": "gulp test-browser",
    "watch": "gulp watch",
    "build": "gulp build && npm run docs && npm run pandoc",
    "quick-build": "gulp quick-build && npm run docs && npm run pandoc",
    "docs": "jsdoc src/app -R README.md -d docs/",
    "pandoc": "pandoc -V geometry:margin=1in -o docs/api/portal-api.pdf src/docs/portal-api.md",
    "coverage": "gulp coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/statgen/raremetal.js.git"
  },
  "keywords": [
    "genetics",
    "gwas"
  ],
  "author": "Ryan Welch <welchr@umich.edu>",
  "contributors": [
    "Andrew Boughton <abought@umich.edu>",
    "Corbin Quick <corbinq@gmail.com>",
    "Hyun Min Kang <hmkang@umich.edu>",
    "Goncalo Abecasis <goncalo@umich.edu>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/statgen/raremetal.js/issues"
  },
  "homepage": "https://github.com/statgen/raremetal.js",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cli-table": "^0.3.1",
    "del": "^3.0.0",
    "glob": "7.1.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.2",
    "gulp-exit": "0.0.2",
    "gulp-filter": "^5.1.0",
    "gulp-istanbul": "^1.1.3",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-mocha": "^5.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-spawn-mocha": "^5.0.1",
    "gulp-uglify": "3.0.0",
    "gulp-uglify-es": "^1.0.1",
    "isparta": "4.0.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.5",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "webpack": "^3.11.0",
    "webpack-stream": "^4.0.3"
  },
  "babelBoilerplateOptions": {
    "entryFileName": "browser.js",
    "mainVarName": "RareMetal"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "cli-table2": "^0.2.0",
    "jStat": "^1.7.1",
    "js-yaml": "^3.11.0",
    "lib-r-math.js": "git://github.com/welchr/libRmath.js.git#npm",
    "node-fetch": "^2.1.2",
    "numeric": "^1.2.6"
  }
